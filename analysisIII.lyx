\begin_layout Standard
Geradengleichung:
\begin_inset Formula 
\begin{align*}
\left(\text{Re}\left(z\right),\text{Im}\left(z\right),0\right) & \stackrel{!}{=}N+\lambda\cdot\left(Z-N\right)=\left(\begin{array}{c}
\lambda x_{1}\\
\lambda x_{2}\\
1+\lambda\left(x_{3}-1\right)
\end{array}\right)
\end{align*}

\end_inset

Es folgt:
\begin_inset Formula 
\begin{align*}
\lambda & =\frac{1}{1-x_{3}}
\end{align*}

\end_inset

Also folgt:
\begin_inset Formula 
\begin{align*}
z & =\frac{x_{1}+\ii x_{2}}{1-x_{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Berechne die Abstandsfunktion in 
\begin_inset Formula $\overline{\mathbb{C}}$
\end_inset

 genauer:
\begin_inset Formula 
\begin{align*}
\abs z^{2} & =\frac{x_{1}^{2}+x_{2}^{2}}{\left(1-x_{3}\right)^{2}}=\frac{1-x_{3}^{2}}{\left(1-x_{3}\right)\left(1+x_{3}\right)}=\frac{1-x_{3}}{1+x_{3}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\abs z^{2}-x_{3}\abs z^{2} & =1+x_{3}\\
x_{3} & =\frac{\abs z^{2}-1}{\abs z^{2}+1}\\
1-x_{3} & =\frac{\abs z^{2}+1-\abs z^{2}+1}{\abs z^{2}+1}=\frac{2}{1+\abs z^{2}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
x_{1} & =\frac{z+\overline{z}}{1+\abs z^{2}} & x_{2} & =\frac{z-\overline{z}}{\ii\left(1+\abs z^{2}\right)}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
d\left(z,z'\right)^{2} & =\left(x_{1}-x_{1}'\right)^{2}+\left(x_{2}-x_{2}'\right)^{2}+\left(x_{3}-x_{3}'\right)^{2}=\\
 & =2-2\cdot\left(x_{1}x_{1}'+x_{2}x_{2}'+x_{3}x_{3}'\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
x_{1}x_{1}'+x_{2}x_{2}'+x_{3}x_{3}' & =\frac{\left(z+\overline{z}\right)\left(z'+\overline{z}'\right)-\left(z-\overline{z}\right)\left(z'-\overline{z}'\right)+\left(\abs z^{2}-1\right)\left(\abs{z'}^{2}-1\right)}{\left(1+\abs z^{2}\right)\left(1+\abs z'^{2}\right)}=\\
 & =\frac{2z\overline{z}'+2\overline{z}z'+\left(\abs z^{2}-1\right)\left(\abs{z'}^{2}-1\right)}{\left(1+\abs z^{2}\right)\left(1+\abs z'^{2}\right)}=\\
 & =\frac{\left(1+\abs z^{2}\right)\left(1+\abs{z'}^{2}\right)-2\abs{z-z'}^{2}}{\left(1+\abs z^{2}\right)\left(1+\abs z'^{2}\right)}
\end{align*}

\end_inset

Es folgt:
\begin_inset Formula 
\begin{align*}
d\left(z,z'\right) & =\frac{2\abs{z-z'}}{\sqrt{\left(1+\abs z^{2}\right)\left(1+\abs{z'}^{2}\right)}}\\
d\left(z,\infty\right) & =\frac{2}{\sqrt{\left(1+\abs z^{2}\right)}}
\end{align*}

\end_inset


\end_layout

