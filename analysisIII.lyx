\begin_inset Formula $x,y>0$
\end_inset

 annehmen.
\begin_inset Formula 
\begin{align*}
\int_{0}^{\infty}e^{-\frac{x}{y}-y^{2}}\dd x & =e^{-y^{2}}\int_{0}^{\infty}e^{-\frac{x}{y}}\dd x=e^{-y^{2}}\left(-ye^{-\frac{x}{y}}\right)\big|_{0}^{\infty}=e^{-y^{2}}\cdot y\\
\int_{0}^{\infty}ye^{-y^{2}}\dd y & =-\frac{1}{2}\int_{0}^{\infty}\frac{\dd}{\dd y}\left(e^{-y^{2}}\right)\dd y=-\frac{1}{2}e^{-y^{2}}\big|_{0}^{\infty}=-\frac{1}{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Section
